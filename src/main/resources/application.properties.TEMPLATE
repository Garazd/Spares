# Create copy of this file in same directory with name "application.properties" and update value of properties

# Database
datasource.driver.class.name=org.postgresql.Driver
datasource.url=url=jdbc:postgresql://HOST:PORT/DATABASE_NAME
database.username=DB_USERNAME
database.password=DB_PASSWORD
jpa.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
jpa.hibernate.hbm2ddl.auto=update
